<%} if(measurements.length > 0){%>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Parameter</th>
      <th>Messwert</th>
      <th>Gemessen am</th>
    </tr>
  </thead>
  <tbody>
    <% measurements.forEach(measurement => { %>
    <tr>
      <td><%= measurement.parametertype_name %></td>
      <td>
        <%= measurement.data[0][1] %> <%
        if(unit_names[measurement.ts_unitsymbol]){ %> <%=
        unit_names[measurement.ts_unitsymbol] %> <%} else { %> <%=
        measurement.ts_unitsymbol %> <% } %>
      </td>
      <td>
        <% const date = new Date(measurement.data[0][0]) %> <%=
        date.toLocaleDateString("de-ch") %> | <%=
        date.toLocaleTimeString("de-ch") %>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
<%}%>
